{
	"name": "regexYbot",
	"version": "0.1.7",
	"type": "module",
	"dependencies": {
		"@grammyjs/auto-retry": "^2.0.2",
		"@grammyjs/commands": "^1.2.0",
		"@grammyjs/hydrate": "^1.6.0",
		"@grammyjs/runner": "^2.0.3",
		"grammy": "1.38.4"
	},
	"devDependencies": {
		"@bun-security-scanner/osv": "^1.0.0",
		"@types/bun": "1.3.5",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^9.39.2",
		"prettier": "^3.7.4",
		"typescript": "^5.0.0"
	},
	"scripts": {
		"dev": "bun run --watch index.ts",
		"main": "bun index.ts",
		"test": "bun test",
		"typecheck": "bun --bun tsc --noEmit",
		"eslint": "bunx eslint .",
		"lint": "bun run eslint",
		"format": "bunx prettier --write .",
		"format:check": "bunx prettier --check .",
		"ltf": "bun run lint && bun run typecheck && bun run format",
		"ltf:check": "bun run lint && bun run typecheck && bun run format:check"
	}
}
